import java.util.Scanner;
class Main
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        if(n%7==0)
        {
            System.out.println("Divisible by 7");
        }
        else
        {
         System.out.println("Not divisible by 7");   
        }
        s.close();
    }
}
