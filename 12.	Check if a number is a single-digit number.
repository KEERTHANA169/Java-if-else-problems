import java.util.Scanner;
class Main
{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        if((n>=-99 && n<=-10)||(n>=10 && n<=99))
        {
            System.out.println(n+" is a two digit");
        }
        else
        {
            System.out.println(n+" is not an two digit");
        }
        s.close();
    }
}
