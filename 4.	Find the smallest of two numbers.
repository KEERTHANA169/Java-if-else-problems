import java.util.Scanner;
class Main
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int n1=s.nextInt();
        int n2=s.nextInt();
        int min=n1;
        if(n2<min)
        {
            System.out.println(n2);
        }
        else
        {
         System.out.println(min);   
        }
        s.close();
    }
}
